// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

enum Role {
  ADMIN
  COMMON
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id
  name       String
  job        String
  email      String    @unique
  password   String
  read_time  BigInt
  role       Role
  created_at DateTime  @default(now())
  updated_at DateTime?

  @@map("user")
}
