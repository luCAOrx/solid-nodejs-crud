{
  "name": "solid-nodejs-crud",
  "version": "1.0.0",
  "description": "Basic CRUD with Node.js using SOLID principles",
  "main": "./src/server.js",
  "scripts": {
    "start:dev": "tsx watch ./src/infra/http/server.ts",
    "test": "tsx --test ./src/domain/**/**/*.spec.ts",
    "test:e2e": "tsx --test ./test/main.e2e-spec.ts",
    "test:watch": "tsx --test --watch ./src/domain/**/**/*.spec.ts",
    "test:watch:e2e": "tsx --test --watch ./test/main.e2e-spec.ts",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "tsx ./prisma/seeds/*.ts"
  },
  "author": "Lucas 'luCAO' Cunha",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "pg": "^8.11.0",
    "prettier": "^2.8.8",
    "prisma": "^5.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  }
}
